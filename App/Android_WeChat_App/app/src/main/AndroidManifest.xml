<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/wechat_logo_icon"
        android:label="WeChat"
        android:roundIcon="@drawable/wechat_logo_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android_WeChat_App"
        tools:targetApi="31">
        <activity
            android:name=".start.AppStart"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".start.welcome" />
    </application>

</manifest>